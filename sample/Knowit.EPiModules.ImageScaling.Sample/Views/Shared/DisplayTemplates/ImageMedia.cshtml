@using Knowit.EPiModules.ImageScaling.Sample.Business.ImageScaling
@model Knowit.EPiModules.ImageScaling.Sample.Models.Media.ImageMedia
@{
    var specifiedPreset = ViewData["ImagePreset"];
    ImagePreset imagePreset;
    if (specifiedPreset == null || !Enum.TryParse(specifiedPreset.ToString(), out imagePreset))
    {
        imagePreset = ImagePreset.NoTransform;
    }

}

<img src="@Url.ScaledImage(Model.ContentLink, imagePreset)" />
